{
  "name": "leapicons-ui",
  "version": "1.0.0",
  "description": "UI for Leapicons",
  "main": "src/index.js",
  "repository": {
    "type": "git"
  },
  "homepage": "https://nishanbajracharya.github.io/leapicons-ui/",
  "author": "Nishan Bajracharya <nisbaj11@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "prestart": "yarn build",
    "lint": "eslint src test",
    "deploy": "gh-pages -d dist",
    "start": "webpack-dev-server",
    "test:coverage": "jest --coverage",
    "lint:fix": "eslint src test --fix",
    "predeploy": "yarn build:production",
    "build": "rimraf dist && webpack --progress --colors",
    "build:production": "rimraf dist && NODE_ENV=production webpack --config ./webpack.config.production.js --progress --colors -p"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.10.8"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "dotenv": "^4.0.0",
    "fuse.js": "^3.1.0",
    "normalize.css": "^7.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.0.0",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.27.3",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-devtools-extension": "^2.13.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|ico|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
